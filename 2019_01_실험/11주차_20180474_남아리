A = [3 8 0;1 2 5]

A =

     3     8     0
     1     2     5

B = [1 2 3 4; 4 5 1 2;0 2 3 0]

B =

     1     2     3     4
     4     5     1     2
     0     2     3     0

C = [ 5 4 5; 7 9 3]

C =

     5     4     5
     7     9     3

3*A

ans =

     9    24     0
     3     6    15

A+C

ans =

     8    12     5
     8    11     8

A-C

ans =

    -2     4    -5
    -6    -7     2

A.*C

ans =

    15    32     0
     7    18    15

A./C

ans =

    0.6000    2.0000         0
    0.1429    0.2222    1.6667

transpose A

ans =

A

clear ans
A transpose
